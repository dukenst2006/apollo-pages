{
  "name": "weerd/apollo-pages",
  "description": "Laravel package to provide static page CMS.",
  "keywords": ["pages", "cms"],
  "license": "MIT",
  "authors": [
    {
      "name": "Diego Lorenzo",
      "email": "diego@diegolorenzo.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Weerd\\ApolloPages\\": "src/"
    }
  },
  "require": {
    "illuminate/database": "^5.3",
    "illuminate/support": "^5.3",
    "illuminate/routing": "^5.3"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Weerd\\ApolloPagesTests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "symfony/var-dumper": "^3.1"
  }
}
