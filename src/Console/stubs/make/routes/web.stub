
/*
| The following Page routes need to be placed at the very end of the
| web route declarations file.
*/

// Admin Routes
Route::group(['namespace' => 'Admin', 'prefix' => 'admin'], function() {
    Route::resource('pages', 'PageController', ['as' => 'admin']);
});

// Client Routes
Route::group(['namespace' => 'Client'], function () {
    Route::get('{params?}', 'PageController@show')->where('params', '(.*)');
});
